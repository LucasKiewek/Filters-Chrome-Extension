{
"manifest_version": 2,
"name": "Web Filters",
"short_name": "Filters",
"description": "Adds a filter to your webpage.",
"version": "1.0.3",

"browser_action": {
"default_icon": "image/icon19.png",
"default_popup": "popup.html" },

"icons": { "16": "image/icon16.png",
"19": "image/icon19.png",
"48": "image/icon48.png",
"128": "image/icon128.png" },

"permissions": [
"activeTab",
"storage" ]
}
